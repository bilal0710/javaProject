CREATE TABLE PERSON (ID BIGINT IDENTITY NOT NULL, CREATED TIMESTAMP, EMAIL VARCHAR, FIRSTNAME VARCHAR, LASTNAME VARCHAR, MODIFIED TIMESTAMP, VERSION BIGINT, PRIMARY KEY (ID))
CREATE TABLE ADDRESSBOOK (ID BIGINT IDENTITY NOT NULL, CREATED TIMESTAMP, MODIFIED TIMESTAMP, VERSION BIGINT, PRIMARY KEY (ID))
CREATE TABLE ADDRESS (ID BIGINT IDENTITY NOT NULL, CITY VARCHAR, CREATED TIMESTAMP, MODIFIED TIMESTAMP, STREET VARCHAR, VERSION BIGINT, ZIPCODE VARCHAR, PRIMARY KEY (ID))
CREATE TABLE ADDRESSBOOK_PERSON (AddressBook_ID BIGINT NOT NULL, persons_ID BIGINT NOT NULL, PRIMARY KEY (AddressBook_ID, persons_ID))
ALTER TABLE ADDRESSBOOK_PERSON ADD CONSTRAINT FK_ADDRESSBOOK_PERSON_AddressBook_ID FOREIGN KEY (AddressBook_ID) REFERENCES ADDRESSBOOK (ID)
ALTER TABLE ADDRESSBOOK_PERSON ADD CONSTRAINT FK_ADDRESSBOOK_PERSON_persons_ID FOREIGN KEY (persons_ID) REFERENCES PERSON (ID)
